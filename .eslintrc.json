{
  "env": {
    "browser": true,
    "es2021": true,
    "commonjs": true
  },
  "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "prettier"
  ],
  "parserOptions": {
      "ecmaFeatures": {
          "jsx": true
      },
      "ecmaVersion": 12,
      "sourceType": "module"
  },
  "plugins": [
      "react", "prettier"
  ],
  "globals": {
    "process": true
  },
  "rules": {
  }
}